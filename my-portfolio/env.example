# Environment Variables Template
# Copy this file to .env and fill in your actual API keys

# Groq API Key for AI chat functionality
VITE_GROQ_API_KEY=your_groq_api_key_here

# Apify API Token for LinkedIn data scraping
VITE_APIFY_API_TOKEN=your_apify_api_token_here

# EmailJS Public Key for contact form
VITE_EMAILJS_PUBLIC_KEY=your_emailjs_public_key_here

# EmailJS Service ID (optional - can be hardcoded if not changing)
VITE_EMAILJS_SERVICE_ID=service_bn14xnr

# EmailJS Template ID (optional - can be hardcoded if not changing)
VITE_EMAILJS_TEMPLATE_ID=template_7r3qp0y 